<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <VersionPrefix>0.1.0</VersionPrefix>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>dotnet-exec</AssemblyName>
    <PackageId>dotnet-exec</PackageId>
    <OutputType>Exe</OutputType>
    <Authors>Jared Nance</Authors>
    <PackageType>DotnetCliTool</PackageType>
    <PackageProjectUrl>https://github.com/jaredcnance/dotnet-exec</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="colorful.console" Version="1.1.2" />
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>
</Project>